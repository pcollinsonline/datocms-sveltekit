{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"endOfLine": "auto",
	"semi": false,
	"singleAttributePerLine": false,
	"bracketSameLine": true,
	"svelteStrictMode": true,
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
